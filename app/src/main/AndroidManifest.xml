        <activity android:name="com.example.jinbailiang.image_about.ImageAboutActivity" />
        <activity android:name="com.example.jinbailiang.otherinfomation.OtherInfoActivity" />
        <activity android:name="com.example.jinbailiang.settings.MySettingsActivity"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
